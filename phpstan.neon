parameters:
    ignoreErrors:
        - message: '#Parameter \#3 \$body of class GuzzleHttp\\Psr7\\Response constructor expects#'
          path: tests/
        - message: '#Parameter .+ of class Lmc\\Matej\\Matej constructor expects string#'
          path: tests/integration/IntegrationTestCase.php
        - message: '#Parameter \#4 \$body of method Http\\Message\\RequestFactory::createRequest\(\) expects#'
          path: src/Http/RequestManager.php
        - '#Unsafe usage of new static\(\)#'
        - message: '#expects class-string#'
          path: tests/
        - message: '#Unable to resolve the template type ExpectedType#'
          path: tests/unit/RequestBuilder/RequestBuilderFactoryTest.php
        - message: '#parameter \$factoryArguments with no typehint specified#'
          path: tests/unit/RequestBuilder/RequestBuilderFactoryTest.php

    checkGenericClassInNonGenericObjectType: false
    checkMissingIterableValueType: false
    level: 7
    paths:
      - src/
      - tests/
